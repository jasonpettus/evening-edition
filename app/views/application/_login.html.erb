<% if session['login_error'] %>
  <p><%= Incorrect Email or Password %></p>
<% end %>

<div class="login_box">
  <%= form_tag("/users/login", method: 'POST', id: 'login-form') do %>
    <%= text_field_tag 'email', nil, placeholder: 'Email' %>
    <%= text_field_tag 'password', nil, placeholder: 'Password' %>
    <%= submit_tag 'Login' %>
  <% end %>
</div>
